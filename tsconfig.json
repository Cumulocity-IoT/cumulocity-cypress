{
  "compilerOptions": {
    "rootDir": "./",
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "lib": ["dom", "dom.iterable", "ES2020"],
    "types": ["cypress", "node"],
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "module": "Node16",
    "skipLibCheck": true
  },
  "include": ["lib/**/*.ts"],
  "exclude": ["cypress", "app", "node_modules"]
}
