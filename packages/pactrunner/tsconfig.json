{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "lib": ["ES2020", "dom"],
    "types": ["cypress", "node"],
    "moduleResolution": "node",
    "composite": true,
    "strictNullChecks": true,
    "strict": true,
    "paths": {
      "cumulocity-cypress/*": ["./../../src/*"],
      "cumulocity-cypress": ["./../../src"]
